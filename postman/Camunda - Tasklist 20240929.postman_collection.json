      "name": "Keycloak - Generate JWT",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "var jsonData = JSON.parse(pm.response.text());\r",
              "pm.environment.set(\"camunda_token\", jsonData[\"access_token\"])"
            ],
            "type": "text/javascript",
            "packages": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/x-www-form-urlencoded"
          }
        ],
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {
              "key": "client_id",
              "value": "{{token_client_id}}",
              "type": "text"
            },
            {
              "key": "client_secret",
              "value": "{{token_client_secret}}",
              "type": "text"
            },
            {
              "key": "grant_type",
              "value": "client_credentials",
              "type": "text"
            }
          ]
        },
        "url": {
          "raw": "http://localhost:18080/auth/realms/camunda-platform/protocol/openid-connect/token",
          "protocol": "http",
          "host": ["localhost"],
          "port": "18080",
          "path": ["auth", "realms", "camunda-platform", "protocol", "openid-connect", "token"]
        },
        "description": "Generated from cURL: curl --location --request POST 'http://localhost:18080/auth/realms/camunda-platform/protocol/openid-connect/token' \\\r\n--header 'Content-Type: application/x-www-form-urlencoded' \\\r\n--data-urlencode \"client_id=${CLIENT_ID}\" \\\r\n--data-urlencode \"client_secret=${CLIENT_SECRET}\" \\\r\n--data-urlencode 'grant_type=client_credentials'"
      },
      "response": []
    },
